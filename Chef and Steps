#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t,n,k;
    
    cin>>t;
    while(t--)
    {
        cin>>n>>k;
        int a[n];
        stringstream res;
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
            
            if(a[i]%k==0)
            res<<"1";
            else
            res<<"0";
        }
        cout<<res.str()<<endl;
    }
   
}
