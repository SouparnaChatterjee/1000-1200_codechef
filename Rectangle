#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t,a,b,c,d;
    cin>>t;
    while(t--)
    {
     cin>>a>>b>>c>>d;
     if(a==b && c==d)
     cout<<"YES"<<endl;
     else if(a==d && b==c)
     cout<<"YES"<<endl;
     else if(a==c && b==d)
     cout<<"YES"<<endl;
     else
     cout<<"NO"<<endl;
    }
}
