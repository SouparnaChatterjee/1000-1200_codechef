#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t,n;
    cin>>t;
    while(t--)
    {
        int count=0;
        cin>>n;
        int a[n];
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
            if(a[i]%2!=0)
            count++;
        }
      if(count>0 && count%2==0) 
      cout<<"YES"<<endl;
      else
      cout<<"NO"<<endl;
    }
}
