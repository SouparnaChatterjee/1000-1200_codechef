#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t,n;
    cin>>t;
    string s;
    while(t--)
    {
        cin>>n;
        int count=0;
        cin>>s;
        for(int i=0;i<n;i++)
        {
            if(s[i]=='a'|| s[i]=='e'|| s[i]=='i'|| s[i]=='o'|| s[i]=='u')
            {
                count=0;
                continue;
            }
        
        else{
            count++;
            if(count==4)
            break;
        }
        
    }
    if(count<4)
        cout<<"yes"<<endl;
        else
        cout<<"no"<<endl;
}
}
