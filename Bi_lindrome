#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t,n;
    string s;
    cin>>t;
    while(t--)
    {
        cin>>n;
        cin>>s;
        unordered_set<int> st;
        for(char c:s)
        st.insert(c);
        cout<<(st.size()<n?n-2:-1)<<endl;
    }
}
