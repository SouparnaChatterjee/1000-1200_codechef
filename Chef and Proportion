#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int a,b,c,d;
   
    {
        cin>>a>>b>>c>>d;
        //always use / 
        if((a*c==b*d) || (a*b==c*d) || (a*b==c*d) ||(a*d==b*c) )
        cout<<"Possible"<<endl;
        else
        cout<<"Impossible"<<endl;
    }
}
